
{{> header}}

{{> navbar }}

<h2>Maquinas registradas</h2>

<table id="maquinas">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Descripcion</th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.nombre}}</td>
            <td>{{this.descripcion}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>

{{> footer }}